
public class BiggerBetterAdder {
		
	    public static void main(String[] args) {
	        int num1 = 5;
	        int num2 = 2;
	        int num3 = 7;
	        
	        int sum = num1 + num2 + num3;
	        
	        System.out.println(num1 + " + " + num2 + " + " + num3 + " = " + sum);
	    }
	

}
